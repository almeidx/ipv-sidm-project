{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"noEmit": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2023",
		"noUncheckedSideEffectImports": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*.ts"]
}
